<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- EPAC -->
        <record id="type_epac" model="purchase.requisition.type">
            <field name="name">EPAC</field>
            <field name="sequence">5</field>
            <field name="quantity_copy">copy</field>
        </record>

        <record id="seq_epac_purchase_requisition" model="ir.sequence">
            <field name="name">EPAC</field>
            <field name="code">purchase.requisition.epac</field>
            <field name="prefix">EPAC/%(y)s.</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range">True</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"></field>
        </record>

        <!-- Marché -->
        <record id="type_marche" model="purchase.requisition.type">
            <field name="name">MARCHE</field>
            <field name="sequence">5</field>
            <field name="quantity_copy">copy</field>
        </record>

        <record id="seq_marche_purchase_requisition" model="ir.sequence">
            <field name="name">MARCHE</field>
            <field name="code">purchase.requisition.marche</field>
            <field name="prefix">MARCHE/%(y)s.</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range">True</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"></field>
        </record>
        <!-- MAPA à BdC -->
        <record id="type_mapa" model="purchase.requisition.type">
            <field name="name">MAPA à BdC</field>
            <field name="sequence">5</field>
            <field name="quantity_copy">copy</field>
        </record>
        <record id="type_mafor" model="purchase.requisition.type">
            <field name="name">Marché à BdC</field>
            <field name="sequence">5</field>
            <field name="quantity_copy">copy</field>
        </record>
    </data>
</odoo>
