<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="complete_expense_od_view_form" model="ir.ui.view">
        <field name="name">complete.expense.od.form</field>
        <field name="model">complete.expense.od</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="company_id" invisible="1" />
                        <field name="currency_id" invisible="1" />
                        <field name="hr_expense_sheet_id" invisible="1" context="{'default_hr_expense_sheet_id': hr_expense_sheet_id}" />
                        <field name="compensation_analytic_tag_ids" groups="analytic.group_analytic_tags" widget="many2many_tags" options="{'no_create': True}" />
                    </group>
                </group>
                <footer>
                    <button name="action_od_lines" type="object" string="Add" class="btn-primary" />
                    <button string="Discard" special="cancel" class="btn-secondary" />
                </footer>
            </form>
        </field>
    </record>

    <record id="complete_expense_od_view" model="ir.actions.act_window">
        <field name="name">Create Automatic Entries for selected Journal Items</field>
        <field name="res_model">complete.expense.od</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo> 